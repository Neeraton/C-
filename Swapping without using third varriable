#include<iostream>
using namespace std;
int main()
{
    int a,b;
    cout<<"Enter the first number:";
    cin>>a;
    cout<<"Enter the second number:";
    cin>>b;
    cout<<"\nBefore swapping:\n";
    cout<<"\nfirst number ="<<a;
    cout<<"\nsecond number ="<<b;

    a=a+b;
    b=a-b;
    a=a-b;

    cout<<"\n\nAfter swapping:\n";
    cout<<"\nfirst number ="<<a;
    cout<<"\nsecond number ="<<b;

}

